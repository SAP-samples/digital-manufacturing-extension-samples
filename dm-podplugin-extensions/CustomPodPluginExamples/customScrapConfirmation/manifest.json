{
	"sap.app": {
		"id": "sap.ext.exampleplugins.customScrapConfirmation",
		"type": "component",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"embeddedBy": "../",
		"title": "{{title}}",
		"description": "{{plugin.description}}"
	},
  
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ext.exampleplugins.customScrapConfirmation.view.MainView",
			"type": "XML"
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ext.exampleplugins.customScrapConfirmation.i18n.i18n"
				}
			}
		}
	}
}